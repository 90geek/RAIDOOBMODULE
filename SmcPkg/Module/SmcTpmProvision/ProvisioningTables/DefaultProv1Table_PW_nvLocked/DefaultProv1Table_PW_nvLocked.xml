
    <!-- PSPolicyData is structured as raw fields. The WORD, BYTE and DWORD tag values are just for user notation-->
    <PSPOLICYDATA POLTYPE="ANY">
      <!-- Version -->
        <WORD VALUE="0x0202"
              SIZE="2">
        </WORD>
      <!-- Hash ALG 0=SHA_1 -->
        <BYTE VALUE="0"
              SIZE="1">
        </BYTE>
      <!-- Policy Type 0=LIST, 1=ANY -->
        <BYTE VALUE="1"
              SIZE="1">
        </BYTE>
      <!-- SINIT Min Version -->
        <BYTE VALUE="0"
              SIZE="1">
        </BYTE>
      <!-- Reserved1 -->
        <BYTE VALUE="0"
              SIZE="1">
        </BYTE>
      <!-- Data Revocation Counter for 1st list -->
        <WORD VALUE="0"
              SIZE="2">
        </WORD>
      <!-- Data Revocation Counter for 2nd list -->
        <WORD VALUE="0"
              SIZE="2">
        </WORD>
      <!-- Data Revocation Counter for 3rd list -->
        <WORD VALUE="0"
              SIZE="2">
        </WORD>
      <!-- Data Revocation Counter for 4th list -->
        <WORD VALUE="0"
              SIZE="2">
        </WORD>
      <!-- Data Revocation Counter for 5th list -->
        <WORD VALUE="0"
              SIZE="2">
        </WORD>
      <!-- Data Revocation Counter for 6th list -->
        <WORD VALUE="0"
              SIZE="2">
        </WORD>
      <!-- Data Revocation Counter for 7th list -->
        <WORD VALUE="0"
              SIZE="2">
        </WORD>
      <!-- Data Revocation Counter for 8th list -->
        <WORD VALUE="0"
              SIZE="2">
        </WORD>
      <!-- Policy control -->
        <!-- 0xE=FO,NPW,PCR17; 0xC=FO,PCR17; 0xA=FO,NPW; 0x8=FO; 0x6=NPW,PCR17; 0x4=PCR17; 0x2=NPW; 0x0=PW -->
              <!-- bit 3= Force Owner Policy                        -->
              <!-- bit 2= Extend OsSinitData.Capabilities in PCR17  -->
              <!-- bit 1= Allow NPW                                 -->              
        <DWORD VALUE="0x0"
              SIZE="4">
        </DWORD>
      <!-- Reserved2[0] -->
        <BYTE VALUE="0"
              SIZE="4">
        </BYTE>
      <!-- Reserved2[1] -->
        <BYTE VALUE="0"
              SIZE="4">
        </BYTE>
      <!-- Policy Hash -->
        <!-- This is the SHA1 hash of 20 bytes -->
        <BYTE VALUE="00"
              SIZE="1">
        </BYTE>
