//## Syntax Highlighting for C and C++

EXT=C,CPP,H,HPP
KEYWORDS=BYTE,DWORD,LONG,PVOID,UCHAR,UINT,ULONG,VOID,WORD
KEYWORDS=asm,auto,break,byte,case,catch,cdecl,char
KEYWORDS=class,const,continue,default,delete,double,do,dword,else,enum
KEYWORDS=extern,far,float,for,friend,goto,huge,if,inline,interrupt
KEYWORDS=int,long,near,new,operator,pascal,private,protected,public
KEYWORDS=register,return,short,signed,sizeof,static,struct,switch
KEYWORDS=template,this,typedef,union,unsigned,virtual,void,volatile,word,while
//## EFI types
KEYWORDS=INT8,INT16,INT32,INT64,INTN,UINT8,UINT16,UINT32,UINT64,UINTN,CHAR8,CHAR16,BOOLEAN,EFI_HANDLE,EFI_STATUS,EFI_GUID
INSTRUCTIONS=include,define,ifdef,ifndef,elif,endif,undef,defined,pragma,TRUE,FALSE,true,false
//## define
TYPE1_REGEXP="^[[:blank:]]*#[[:blank:]]*define[[:blank:]]+([[:word:]]+)"
//## function
TYPE2_REGEXP="^[[:blank:]]*[[:word:]]+[[:space:]\*]*[[:space:]]+([[:word:]]+)[[:space:]]*\([^;]+?\{"
//## typedef of structure or enum
SYMDEF="^[[:blank:]]*typedef[[:space:]]+(?:struct|enum|union)[[:space:]]*[[:word:]]*[[:space:]]*\{(?:[[:word:][:space:][:punct:]\*]+?)\}[[:space:]]*([[:word:]]+)"
//## typedef
SYMDEF="^[[:blank:]]*typedef[[:space:]]+(?:[[:word:]]+[[:space:]]+)?[[:word:]]+[[:space:]\*]+(?:\((?:[[:space:]]*[[:word:]]+)?[[:space:]\*]*\*)?([[:word:]]+)"
EOLCOMMENTS=//
OPENCOM=/*
CLOSECOM=*/
SENSITIVE=1

