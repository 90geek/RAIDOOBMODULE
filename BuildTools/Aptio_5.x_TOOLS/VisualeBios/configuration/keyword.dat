KEYWORDS=Question,EndQuestion,MenuItem,EndMenuItem,ControlGroup,EndControlGroup
KEYWORDS=Separator,EndSeparator,DynamicText,EndDynamicText,StaticText,EndStaticText
KEYWORDS=DynamicText,EndDynamicText,Control
KEYWORDS=DateControl,EndDateControl,NvramField,EndNvramField,TimeControl,EndTimeControl
