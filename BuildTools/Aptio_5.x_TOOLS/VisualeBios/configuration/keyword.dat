//## Syntax Highlighting for 80x86 Assembly

EXT=ASM,INC,EQU,DAT,CSS,MAC
KEYWORDS=BYTE,CODEPTR,DATAPTR,DUP,DWORD,ENDM,ENDP,ENDS,EQ,EXTRN,EXTERN,FAR,END
KEYWORDS=FWORD,GE,GT,HIGH,LARGE,LENGTH,LE,LOW,LT,MACRO,MASK,MOD,NEAR,NE
KEYWORDS=OFFSET,PROC,PTR,PUBLIC,PWORD,QWORD,SEGMENT,SEG,SHORT,LABEL,EQU
KEYWORDS=SIZE,SMALL,STRUC,SYMTYPE,TBYTE,THIS,TYPE,UNKNOWN,WIDTH,WORD
KEYWORDS=ASSUME,COMMENT,ENDOFCOMMENT,'CODE',TITLE,INCLUDE,NOTHING
KEYWORDS=IF,ELSE,ENDIF,PRIVATE
KEYWORDS=BIT0,BIT1,BIT2,BIT3,BIT4,BIT5,BIT6,BIT7,BIT8,BIT9,BIT10,BIT11,BIT12,BIT13,BIT14,BIT15
KEYWORDS=AL,BL,CL,DL,AH,BH,CH,DH,AX,BX,CX,DX,SI,DI,BP,SP,EAX,EBX,ECX,EDX,ESI,EDI,EBP,ESP,CS,DS,SS,ES,FS,GS,RAX,RBX,RCX,RDX,RSI,RDI,RBP,RSP
INSTRUCTIONS=AAA,AAD,AAM,AAS,ADC,ADD,AND,ARPL
INSTRUCTIONS=BOUND,BSF,BSR,BSWAP,BT,BTC,BTR,BTS
INSTRUCTIONS=CBQ,CDQ,CLC,CLD,CLI,CLTS,CMC,CPUID,CMOV,CMP,CMPS,CMPSB,CMPSD,CMPSW,CMPXCHG,CMPXCHG8B,CWD,CWDE,CALL
INSTRUCTIONS=DAA,DAS,DEC,DIV,EMMS,ENTER,STC
INSTRUCTIONS=F2XM1,FABS,FADD,FADDP,FBLD,FBSTP,FCHS,FCLEX
INSTRUCTIONS=HLT,IDIV,IMUL,IN,INC,INS,INSB,INSD,INSW,INT,IRET,IRETD
INSTRUCTIONS=JA,JAE,JB,JBE,JC,JCXZ,JECXZ,JE,JG,JGE,JL,JLE,JNA,JNAE,JNB,JNBE,JNC,JNE,JMP,JZ,JNZ,LOOP
INSTRUCTIONS=MOV,MOVZX,NEG,PUSH,POP,PUSHA,POPA,PUSHAD,POPAD,PUSHF,POPF,RET,RETF
INSTRUCTIONS=SHR,SHL,SUB,OR,XOR,TEST,OUT,XCHG
EOLCOMMENTS=;
EOLCOMMENTS=#
TYPE1_REGEXP="^[[:blank:]]*([[:alnum:]_@]+)[[:blank:]]+SEGMENT"
TYPE2_REGEXP="^[[:blank:]]*([[:alnum:]_@]+)[[:blank:]]*:"
TYPE2_REGEXP="^[[:blank:]]*([[:alnum:]_@]+)[[:blank:]]+PROC"
SYMDEF="^[[:blank:]]*([[:alnum:]_@]+)[[:blank:]]+(?:(?:TEXT)?EQU)|="
SYMDEF="^[[:blank:]]*([[:alnum:]_@]+)[[:blank:]]+(?:(?:TEXT)?STRUC)|="
SYMDEF="^[[:blank:]]*([[:alnum:]_@]+)[[:blank:]]+(?:(?:TEXT)?LABEL)|="
SYMDEF="^[[:blank:]]*([[:alnum:]_@]+)[[:blank:]]+DB"
SYMDEF="^[[:blank:]]*([[:alnum:]_@]+)[[:blank:]]+DW"
SYMDEF="^[[:blank:]]*([[:alnum:]_@]+)[[:blank:]]+DD"
SENSITIVE=0
